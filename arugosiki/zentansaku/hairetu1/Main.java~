import java.util.*;
public class Main{
	public static void main(String[] args){
		Scanner scan = new Scanner(System.in);
		int n = scan.next();
		int v = scan.next();
		boolean f = false;
		for(int i = 0;i<n;i++){
			int a = scan.next();
			if(a == v){
				f = true;
				System.out.println("Yes");
				break;
			}
		}
		if(f == false){
			System.out.println("No");
		}

		scan.close();
	}
}

